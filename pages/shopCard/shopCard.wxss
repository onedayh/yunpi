page{
    box-sizing: border-box;
    padding: 30rpx 30rpx 130rpx;
}
view, scroll-view{
    box-sizing: border-box;
}
.wrap{
    border-radius: 16rpx;
    background: #fff url("data:image/png;base64,") no-repeat left top/100% 150rpx;
    box-shadow: 0 0 20rpx rgba(0, 0, 0, .1);
    padding: 50rpx 40rpx 0;
}
.name{
    font-size: 36rpx;
    color: #fff;
    height: 60rpx;
    display: flex;
    align-items: center;
}
.name image{
    width: 60rpx;
    height: 60rpx;
    margin-right: 20rpx;
}
.list{
    margin-top: 40rpx;
    padding-top: 30rpx;
}
.item{
    margin-bottom: 40rpx;
}
.item:last-child{
    margin-bottom: 20rpx;
}
.item view{
    display: flex;
    align-items: center;
    justify-content: space-between;
    color: #999;
    font-size: 24rpx;
    line-height: 40rpx;
}

.item .handle{
    color: orangered;
}
.more{
    display: flex;
    justify-content: center;
    align-items: center;
    height: 40rpx;
}
.more image{
    width: 26rpx;
    height: 16rpx;
}
.b-wrap{
    margin-top: 40rpx;
}
.b{
    display: flex;
    flex-direction: column;
    align-items: baseline;
}
.b-item{
    font-size: 24rpx;
    color: #333;
    background-color: #fff;
    border-radius: 8rpx;
    padding: 9rpx 20rpx;
    margin-bottom: 20rpx;
}
.b-item text{
    color: #999;
}






.user-set{
    height: 100rpx;
    display: flex;
    width: 100%;
    justify-content: space-around;
    font-size: 28rpx;
    align-items: center;
    position: fixed;
    left: 0;
    bottom: 0;
    background-color: #fff;
    color: #666;
    z-index: 10;
}
.user-set view{
    display: flex;
    height: 100rpx;
    width: 50%;
    justify-content: center;
    align-items: center;
}
.user-set text{
    width: 2rpx;
    height: 36rpx;
    background-color: #e6e6e6;
    position: absolute;
    top: 34rpx;
    left: 50%;
}