
page {
    padding-bottom: 80rpx;
}
.cart-box {
    padding: 20rpx 20rpx 0 20rpx;
    background-color: #f5f5f5;
}

.item {
    margin-bottom: 25rpx;
    border-radius: 4px;
    overflow: hidden;
    background-color: #fff;
    position: relative;
}
.item-wrap{
    overflow: hidden;
    padding: 10px;
    -webkit-transition: all .2s;
    transition: all .2s;
}
.delete{
    position: absolute;
    right: 0rpx;
    top: 0;
    width: 128rpx;
    height: 240rpx;
    background: linear-gradient(to right, orangered, #f33);
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 26rpx;
    color: #fff;
    -webkit-transition: all .2s;
    transition: all .2s;
    -webkit-transform: translateX(128rpx);
    transform: translateX(128rpx);
}
.del .item-wrap{
    -webkit-transform: translateX(-128rpx);
    transform: translateX(-128rpx);
}
.del .delete{
    -webkit-transform: translateX(0);
    transform: translateX(0);
}

.iconName {
    float: left;
    margin-top: 82rpx;
    width: 36rpx;
    height: 36rpx;
}

.goodsImg {
    width: 200rpx;
    height: 200rpx;
    float: left;
    margin-left: 10rpx;
}

.goodsImgs {
    width: 100%;
    height: 100%;
    float: left;
}

.goods-info {
    width: 400rpx;
    height: 200rpx;
    float: right;
    box-sizing: border-box;
}

.goods-name {
    color: #000;
    font-size: 26rpx;
    padding: 6rpx 0;
    /*min-height: 68rpx;*/
}

.prContent {
    overflow: hidden;
    color: #999;
    font-size: 24rpx;
    margin-top: 20rpx;
}

.goodsColor {
    float: left;
}

.size {
    float: left;
    margin-left: 30rpx;
}

.pcContent {
    overflow: hidden;
    margin-top: 20rpx;
}

.goods-price {
    float: left;
    font-weight: Bold;
    color: #f33;
    font-size: 28rpx;
}

.btn-groups {
    float: right;
    overflow: hidden;
}
.num {
    float: left;
    min-width: 50rpx;
    padding: 0 10rpx;
    height: 50rpx;
    text-align: center;
    line-height: 50rpx;
    color: #000;
    font-size: 28rpx;
}

.btn-add, .btn-minus {
    border: 1rpx solid #f2f2f2;
    float: left;
    width: 50rpx;
    height: 50rpx;
    text-align: center;
    line-height: 50rpx;
    color: #161616;
	font-weight:bold;
}

#allprice {
    color: #666;
    /* font-size: 24rpx; */
}

.pricee {
    font-weight: bold;
}

.cart-fixbar {
    position: fixed;
    bottom: 0;
    height: 100rpx;
    width: 100%;
    padding: 0 20rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    background-color: #f5f5f5;
}

.cart-fixbar .allSelect {
    display: flex;
    flex-direction: row;
    height: 100rpx;
    align-items: center;
    color: #000;
    font-size: 26rpx;
}

.cart-fixbar .allSelect-text {
    margin-left: 20rpx;
}

.cart-fixbar .count {
    margin-left: 50rpx;
    color: #fff;
    font-size: 26rpx;
}

.cart-fixbar .order {
    color: #fff;
    height: 72rpx;
    line-height: 72rpx;
    width: 212rpx;
    background: linear-gradient(to right, orangered, #f33);
    border-radius: 36rpx;
    font-size: 28rpx;
    text-align: center;
}

.cart-fixbar .allnum {
    font-size: 24rpx;
}

.allprice {
    color: #f33;
}

.disa{
    background-color: #f5f5f5;
    color: #aaa;
}
.empty{
    font-size: 30rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}
.liulan{
    width: 200rpx;
    height: 70rpx;
    border-radius: 8rpx;
    background-color: orangered;
    color: #fff;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: 100rpx;
}



.user{
    background-color: #fff;
    padding: 100rpx 0 0 20rpx;
    box-sizing: border-box;
}
.msg-top{
    height: 100rpx;
