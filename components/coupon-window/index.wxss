.wwrap{
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background-color: rgba(0, 0, 0, .6);
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    z-index: 11;
}
.wrap{
    box-sizing: border-box;
    width: 550rpx;
    height: 600rpx;
    padding: 0 26rpx;
    background: url('data:image/png;base64,') no-repeat left top/100% 100%;
    display: flex;
    position: relative;
    flex-direction: column;
    align-items: center;
}
.cha{
    width: 68rpx;
    height: 226rpx;
    vertical-align: top;
}
.title{
    color: #fff;
    font-size: 36rpx;
    padding: 60rpx 0 50rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.tips{
    font-size: 24rpx;
    color: #fff;
    padding-bottom: 20rpx;
}

.item{
    width: 100%;
    box-sizing: border-box;
    margin-bottom: 20rpx;
    border-radius: 16rpx;
    background-color: #fff;
    position: relative;
    display: flex;
    align-items: center;
    padding: 0 20rpx;
    justify-content: space-around;
    height: 140rpx;
    overflow: hidden;
}
.item::before, .item::after{
    content: '';
    width: 12rpx;
    height: 12rpx;
    border-radius: 50%;
    position: absolute;
    top: 64rpx;
}
.item::before{
    left: -6rpx;
    background-color: #ff4d18;
}
.item::after{
    right: -6rpx;
    background-color: #ff3432;
}
.jian{
    font-size: 16rpx;
    color: #fff;
    background-color: orangered;
    height: 22rpx;
    width: 100rpx;
    position: absolute;
    top: 15rpx;
    text-align: center;
    left: -30rpx;
    line-height: 22rpx;
    transform: rotate(-45deg);
}
.money{
    display: flex;
    color: orangered;
    align-items: center;
}
.fu{
    font-size: 24rpx;
    align-self: flex-end;
    padding-bottom: 10rpx;
}
.num{
    font-size: 56rpx;
}
.ts{
    color: orangered;
    font-size: 16rpx;
}
.name{
    color: #333;
    font-size: 24rpx;
}
.time{
    color: #999;
    font-size: 16rpx;
    padding-top: 10rpx;
}
.btn{
    width: 112rpx;
    height: 44rpx;
    background-color: #fff;
    border-radius: 22rpx;
    border: 2rpx solid orangered;
    display: flex;
    align-items: center;
    justify-content: center;
    color: orangered;
    font-size: 20rpx;
    margin: 0;
    padding: 0;
}
.btn::after{
    border: none;
}
.one-box{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    padding-bottom: 10rpx;
}
.order-btn{
    margin-top: 100rpx;
    width: 360rpx;
    height: 88rpx;
    background-color: #FF8433;
    border-radius: 44rpx;
    font-size: 36rpx;
    color: #fff;
}
.ccc{
    width: 68rpx;
    height: 68rpx;
    background-color: transparent;
    position: absolute;
    bottom: -226rpx;
    left: 50%;
    transform: translateX(-34rpx);
    z-index: 2;
}