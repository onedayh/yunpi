    padding-left: 10rpx;
    background: #F2F2F2;
    font-size: 28rpx;
    line-height: 1;
    color: #333;
    display: flex;
    align-items: center;
}
.input input{
    width: 100%;
}
.friends{
    width: 120rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.friends image{
    width: 48rpx;
    height: 48rpx;
}
.friends text{
    color: #888;
    font-size: 20rpx;
}
.search{
    width: 48rpx;
    height: 48rpx;
}
.pc{
    color: #888;
    font-size: 28rpx;
}

.cus-wrap{
    background-color: #fff;
}

.item2{
    background-color: #fff;
    height: 140rpx;
    padding-right: 20rpx;
    border-bottom: 2rpx solid #e6e6e6;
    display: flex;
    align-items: center;
    position: relative;
}
.ac-item2::after{
    content: '';
    width: 16rpx;
    height: 16rpx;
    border-radius: 50%;
    background-color: orangered;
    position: absolute;
    top: 30rpx;
