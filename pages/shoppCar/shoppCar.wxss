    width: 110rpx;
    margin: 4rpx 0 0 6rpx;
    font-size: 16rpx;
    height: 32rpx;
    line-height: 32rpx;
    color: #CA941E;
    text-align: right;
    padding-right: 10rpx;
    background: url(data:image/png;base64,) no-repeat right bottom/110rpx 32rpx;
}
.time1{
    font-size: 24rpx;
    color: #999;
    line-height: 40rpx;
    width: 600rpx;
    padding-top: 10rpx;
}
.create-time{
    position: absolute;
    right: 20rpx;
    top: 34rpx;
    color: #999;
    font-size: 24rpx;
}
.send-btn{
    width: 750rpx;
    height: 144rpx;
    background: transparent;
    position: absolute;
    left: -20rpx;
    top: -2rpx;
