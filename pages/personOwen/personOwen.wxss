page{
    box-sizing: border-box;
    min-height: 100%;
    padding-bottom: 200rpx;
    position: relative;
}
view{
    box-sizing: border-box;
}
.top{
    padding: 40rpx 20rpx;
    height: 240rpx;
    display: flex;
    background: url('data:image/png;base64,') no-repeat left top/100% 240rpx;
}
.top-user{
    background:linear-gradient(180deg, #ff6262 0%, #f33 100%);
}
.top image{
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
}
.top2{
    padding: 15rpx 0 0 30rpx;
    box-sizing: border-box;
}
.name{
    font-size: 32rpx;
    color: #fff;
	height: 42rpx
}
.set{
    font-size: 26rpx;
    color: #fff;
    padding-top: 10rpx;
}
.con{
    padding: 0 20rpx;
    margin-top: -40rpx;
}
.item{
    background-color: #fff;
    border-radius: 8rpx;
    margin-bottom: 20rpx;
}
.all{
    height: 89rpx;
    border-bottom: 1rpx solid #f0f0f0;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 30rpx;
}
.all1{
    color: #333;
    font-size: 30rpx;
    font-weight: bold;
}
.all2{
    color: #666;
    font-size: 26rpx;
    height: 88rpx;
    display: flex;
    align-items: center;
    padding-right: 40rpx;
    background: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAA4AAAAYCAYAAADKx8xXAAAAiUlEQVQ4T63UwQ3AIAgFUBiuR2eqzsSp6XA2QmI0rQJWL158QsxHBAAgus+yh3CkslsWCspRDmO0YiS6IgByRQ9GadWPGa7gCr24gx78glb8CS14CDU8hTOswhE2wjaW5aqcVDhK1f7H0fK7LwBapTp87bRbEY/8r7HyVKqtriBudfmzkiz6v8cHhTZ3WEgCtg4AAAAASUVORK5CYII=') no-repeat right center/14rpx 24rpx;
}
.icon{
    height: 160rpx;
    display: flex;
    align-items: center;
}
.icon-item{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    width: 20%;
    height: 160rpx;
    position: relative;
}
.icon-item image{
    width: 48rpx;
    height: 48rpx;
}
.text{
    color: #333;
    font-size: 26rpx;
    margin-top: 20rpx;
}
.tips{
    height: 32rpx;
    min-width: 32rpx;
    border-radius: 16rpx;
    background-color: orangered;
    color: #fff;
    font-size: 24rpx;
    position: absolute;
    top: 20rpx;
    right: 15rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 0 6rpx;
}
.nav-item{
    padding-left: 20rpx;
	height: 100rpx;
    position: relative;
}
.nav-it{
    height: 100rpx;
    border-bottom: 1rpx solid #e6e6e6;
    display: flex;
    align-items: center;
    background: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAA4AAAAYCAYAAADKx8xXAAAAiUlEQVQ4T63UwQ3AIAgFUBiuR2eqzsSp6XA2QmI0rQJWL158QsxHBAAgus+yh3CkslsWCspRDmO0YiS6IgByRQ9GadWPGa7gCr24gx78glb8CS14CDU8hTOswhE2wjaW5aqcVDhK1f7H0fK7LwBapTp87bRbEY/8r7HyVKqtriBudfmzkiz6v8cHhTZ3WEgCtg4AAAAASUVORK5CYII=') no-repeat 640rpx center/14rpx 24rpx;
}
.nav-it image{
    width: 44rpx;
    height: 44rpx;
    margin-right: 20rpx;
}
.nav-it view{
    color: #333;
    font-size: 28rpx;
}
.item .nav-item:last-child .nav-it{
    border-bottom: none;
}
.form-notice{
    position: absolute;
    width: 100%;
    height: 100rpx;
}
.q{
    height: 100rpx;
}
.point{
    left: 180rpx;
    top: 28rpx;
}
.nav-it .tips-text{
    position: absolute;
    right: 60rpx;
    color: #aaa;
    top: 28rpx;
}
.share{
    display: flex;
    align-items: center;
    margin-left: auto;
    color: #fff;
    font-size: 28rpx;
}
.share image{
    width: 32rpx;
    height: 32rpx;
    border-radius: 0;
    margin-right: 10rpx;
}
.share-wrap{
    width: 100%;
    height: 100%;
    z-index: 100;
    background-color: rgba(0, 0, 0, .5);
    position: fixed;
    left: 0;
    top: 0;
}
.share-w{
    width: 100%;
    position: absolute;
    bottom: -200px;
    background-color: #e6e6e6;
}
.aa-btn{
    margin: 0;
    font-size: 28rpx;
    color: #333;
    width: 620rpx;
    text-align: left;
}
.share-w button, .aa-btn{
    background-color: #fff;
    padding: 0;
    border-radius: 0;
}
.share-w button::after, .aa-btn::after{
    border: none;
}
.share-w .mid-btn{
    margin-top: 1px;
    margin-bottom: 3px;
}
.nextStep {
    width: 590rpx;
    background: linear-gradient(to right, orangered, #f33);
    border-radius: 50rpx;
    color: #fff;
    font-size: 32rpx;
    position: absolute;
    bottom: 20rpx;
    left: 50%;
    margin-left: -295rpx;
}


