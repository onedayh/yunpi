/* pages/choosePayment/choosePayment.wxss */
page{
	box-sizing: border-box;
	padding-bottom: 200rpx;
}
.content {
    padding: 20rpx 20rpx 40rpx;
	background-color: #fff; 
}

.allgoods {
    font-size: 28rpx;
    color: #333;
    margin-left: 20rpx;
}

.orderMoney {
    text-align: center;
    color: #333;
    font-weight: bold;
    font-size: 36rpx;
	height: 120rpx;
	line-height: 120rpx
}

.choseBank {
    display: flex;
    align-items: center;
}

.choseBank .select {
    flex: 0 0 80rpx;
}

.choseBank{
    overflow: hidden;
    margin-bottom: 20rpx;
}
.choseBank:last-child{
  margin-bottom:0;
}

.check {
    width: 36rpx;
    height: 36rpx;
    border: 1rpx solid #ff847f;
    border-radius: 50%;
    margin-left: 10rpx;
}


.bankDetails{
    width: 634rpx;
    height: 198rpx;
    box-sizing: border-box;
    border-radius: 25rpx;
    padding: 20rpx;
    font-size: 26rpx;
    color: #fff;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    background-repeat: no-repeat;
    background-position: 430rpx center;
    background-size: 180rpx 135rpx;
}
.bankpay{
    background-color: #FF847F;
    background-image: url('data:image/png;base64,');
}
.alipay{
    background-color: #43B1E4;
    background-image: url('data:image/png;base64,');
}

.wxpay {
    background-color: #6BC28F;
    background-image: url('data:image/png;base64,');
}


.footer-fixed{
    position: fixed;
    left:0;
    bottom:0;
    width: 100%;
    height: 200rpx;
    background-color: #f5f5f5;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;

}
.chosepayfor {
    font-size: 32rpx;
    height: 88rpx;
    width: 590rpx;
    text-align: center;
    line-height: 88rpx;
    border-radius: 50rpx;
    background: linear-gradient(to right, #ff4e17, #f33);
    color: #fff;
}
.tips{
    font-size:28rpx;
    color:#ccc;
    padding-bottom: 20rpx;
}
.icon {
    width: 36rpx;
    height: 36rpx;
    margin-left: 10rpx;
}

.icon image {
    width: 100%;
    height: 100%;
}
